  "author": "Dusan Brankov <dusanbrankov@proton.me>",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch ./src/scss:dist/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map ./src/scss:dist/css",
    "build:eleventy": "eleventy",
    "start": "rm -r dist/ && npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "postbuild": "postcss ./dist/css/*.css -u postcss-preset-env cssnano -r --no-map"
  },
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@sindresorhus/slugify": "^2.2.0",
    "cssnano": "^5.1.14",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^8.0.1",
    "sass": "^1.57.1"
  }
}
